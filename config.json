{
    "data": {
        "image_size": [
            256,
            256
        ],
        "batch_size": 8,
        "num_workers": 4,
        "train_ratio": 0.8,
        "val_ratio": 0.1,
        "test_ratio": 0.1
    },
    "model": {
        "name": "unetpp",
        "encoder": "resnet34",
        "encoder_weights": "imagenet",
        "activation": "sigmoid"
    },
    "training": {
    "epochs": 10,
    "batch_size": 8,
    "learning_rate": 1e-4,
    "k_folds": 5,
    "early_stop_patience": 10,
    "weight_decay": 1e-5,
    "device": "mps",
    "loss_type": "combined",
    "alpha_schedule": "rebalance",
    "initial_alpha": 0.1,
    "alpha_increment": 0.01,
    "ema_decay": 0.999,
    "consistency_weight": 10.0,
    "consistency_rampup": 10
    },
    "augmentation": {
        "train_prob": 0.5,
        "weak_prob": 0.3,
        "strong_prob": 0.5
    },
    "pseudo_label": {
        "confidence_threshold": 0.9,
        "min_size": 100
    },
    "paths": {
        "data_dir": "data",
        "labeled_dir": "data/annotated",
        "unlabeled_dir": "data/video",
        "pseudo_dir": "data/pseudo_labels",
        "model_dir": "models",
        "log_dir": "logs",
        "checkpoint_dir": "models/checkpoints",
        "scripts_dir": "tools/scripts"
    }
}